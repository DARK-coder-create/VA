{
    "name": "logger",
    "version": "1.0.0",
    "active": 1,
    "use_logger": 1,
    "level": -1,
    "logging": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "level": 10,
        "file_name": false,
        "path_dir_save": "log/",
        "file_extension": "log"
    },
    "data_input": [
        "status",
        "config"
    ],
    "output": [
        "output",
        "logger"
    ]
}