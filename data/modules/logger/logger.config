{
	"name": "logger",
	"version": "1.0.0",

    "active": 1,

	"print_to_console": 1,
	"print_to_file": 1,

	"level": -1,

	"logging": {
		"format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
		"level": 30,
		"file_name": false,
		"path_dir_save": "log/",
		"file_extension": "log"

	},

	"data_input": ["status", "config"],

	"output": ["logger"]
}
